<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="340dp"
    android:layout_height="165dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_centerHorizontal="true"
    android:layout_gravity="center"
    android:layout_marginTop="150dp"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageView3"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:background="@drawable/call"
        android:foregroundGravity="left" />

    <com.firebug.scootr.Views.MyTextView
        app:edittextfont="NEXA LIGHT.OTF"
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:paddingTop="15dp"
        android:text="Rajat Sharma"
        android:textColor="#000000"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/track"
        android:paddingRight="20dp"
        android:layout_below="@+id/textView5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp" />

    <com.firebug.scootr.Views.MyTextView
        app:edittextfont="NEXA LIGHT.OTF"
        android:id="@+id/textView6"
        android:paddingTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignTop="@+id/imageView2"
        android:text="#16 Byrappa Gardens," />

    <com.firebug.scootr.Views.MyTextView
        app:edittextfont="NEXA LIGHT.OTF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ramchandrapura, Jalahalli Post"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <com.firebug.scootr.Views.MyTextView
        app:edittextfont="NEXA LIGHT.OTF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Banglore, 560072"
        android:id="@+id/textView8"
        android:layout_below="@+id/imageView2"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <com.firebug.scootr.Views.MyTextView
        app:edittextfont="NEXA LIGHT.OTF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+91 8734546543"
        android:paddingTop="5dp"
        android:textColor="#000000"
        android:id="@+id/textView9"
        android:layout_below="@+id/textView8"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <com.firebug.scootr.Views.MyTextView
        android:layout_width="match_parent"
        android:layout_height="28dp"
        android:textSize="15sp"
        android:text="Collected Rs.1760 On delivery"
        android:gravity="center"
        app:edittextfont="NEXA LIGHT.OTF"
        android:textColor="#FFFFFF"
        android:background="#FFEC3630"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp" />

    <com.firebug.scootr.Views.MyTextView
        app:edittextfont="NEXA LIGHT.OTF"
        android:paddingTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Slide down to see less"
        android:id="@+id/textView11"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

</RelativeLayout>